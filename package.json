{
  "name": "climbing-wall",
  "version": "1.0.0",
  "description": "A route-tracking app for The Climbing Wall",
  "repository": "git@github.com:bmatcuk/climbing-wall.git",
  "author": "Bob Matcuk <bmatcuk@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production parcel build src/index.html",
    "clean": "rimraf dist .parcel-cache",
    "start": "cross-env NODE_ENV=development parcel src/index.html"
  },
  "lint-staged": {
    "**/*.{?(m)j,t}s?(x)": "eslint --fix"
  },
  "alias": {
    "preact/jsx-dev-runtime": "preact/jsx-runtime"
  },
  "devDependencies": {
    "@types/node": "~17.0.21",
    "@typescript-eslint/eslint-plugin": "~5.14.0",
    "@typescript-eslint/parser": "~5.14.0",
    "cross-env": "~7.0.3",
    "eslint": "~8.11.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-prettier": "~4.0.0",
    "eslint_d": "~11.1.1",
    "lint-staged": "~12.3.5",
    "parcel": "~2.3.2",
    "postcss": "^8.0.0",
    "postcss-modules": "^4.3.0",
    "prettier": "~2.5.1",
    "rimraf": "~3.0.2",
    "typescript": "~4.6.2"
  },
  "dependencies": {
    "preact": "~10.6.6",
    "tslib": "~2.3.1",
    "wouter-preact": "~2.8.0-alpha.2"
  }
}
